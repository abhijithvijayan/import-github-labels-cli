{
	"name": "import-github-labels",
	"version": "1.3.0",
	"description": "CLI to import labels from another repository on GitHub",
	"license": "MIT",
	"repository": "https://github.com/abhijithvijayan/import-github-labels-cli.git",
	"author": {
		"name": "abhijithvijayan",
		"email": "34790378+abhijithvijayan@users.noreply.github.com",
		"url": "https://abhijithvijayan.in"
	},
	"bin": "./dist/index.js",
	"files": [
		"/dist",
		"demo.gif"
	],
	"engines": {
		"node": ">=10.0.0"
	},
	"scripts": {
		"build": "tsc",
		"release": "yarn build && npm publish",
		"dev": "tsc --watch",
		"lint": "eslint . --ext .js,.ts",
		"lint:fix": "eslint . --ext .js,.ts --fix"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"yarn run lint:fix"
		]
	},
	"keywords": [
		"github",
		"repository",
		"lables",
		"importer",
		"repo",
		"cli",
		"cli-app"
	],
	"dependencies": {
		"@octokit/rest": "16.43.1",
		"chalk": "^4.0.0",
		"inquirer": "^7.1.0",
		"meow": "^6.1.0",
		"ora": "^4.0.3"
	},
	"devDependencies": {
		"@types/inquirer": "^6.5.0",
		"@types/node": "^13.11.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"babel-eslint": "10.1.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-onepass": "2.2.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-plugin-html": "^6.0.1",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^3.0.0",
		"husky": "^4.2.3",
		"lint-staged": "^10.1.2",
		"prettier": "^2.0.4",
		"typescript": "^3.8.3"
	}
}
