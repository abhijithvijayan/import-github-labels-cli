{
	"name": "import-github-labels",
	"version": "1.1.0",
	"description": "CLI to import labels from another repository on GitHub",
	"license": "MIT",
	"repository": "https://github.com/abhijithvijayan/import-github-labels-cli.git",
	"author": {
		"name": "abhijithvijayan",
		"email": "34790378+abhijithvijayan@users.noreply.github.com",
		"url": "https://abhijithvijayan.in"
	},
	"bin": "./dist/index.js",
	"files": [
		"/dist"
	],
	"engines": {
		"node": ">=8 <=12"
	},
	"scripts": {
		"build": "tsc",
		"release": "yarn build && npm publish",
		"dev": "tsc --watch",
		"tsc": "tsc",
		"lint": "eslint . --ext .js,.ts",
		"lint:fix": "eslint . --ext .js,.ts --fix"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"npm run lint:fix"
		]
	},
	"keywords": [
		"github",
		"repository",
		"lables",
		"importer",
		"repo",
		"cli",
		"cli-app"
	],
	"dependencies": {
		"@octokit/rest": "^16.38.1",
		"@types/inquirer": "^6.5.0",
		"chalk": "^3.0.0",
		"inquirer": "^7.0.3",
		"meow": "^6.0.0",
		"ora": "^4.0.3"
	},
	"devDependencies": {
		"@types/node": "^13.1.8",
		"@typescript-eslint/eslint-plugin": "^2.17.0",
		"@typescript-eslint/parser": "^2.17.0",
		"babel-eslint": "10.0.3",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-onepass": "2.1.0",
		"eslint-config-prettier": "^6.9.0",
		"eslint-plugin-html": "^6.0.0",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.18.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"husky": "^4.2.0",
		"lint-staged": "^10.0.2",
		"prettier": "^1.19.1",
		"typescript": "^3.7.5"
	}
}
