{
	"parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
	"extends":[ "onepass",     "plugin:@typescript-eslint/eslint-recommended"],
	"rules": {
		"no-console": 0,
		"no-extend-native": 0,
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "es5",
				"singleQuote": true,
				"printWidth": 120,
				"tabWidth": 4
			}
		]
	},
	"settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
